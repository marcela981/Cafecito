{
    "version": 2,
    "routes": [
      {
        "src": "/",
        "dest": "/index",
        "headers": {
          "Cache-Control": "public, max-age=3600, stale-while-revalidate=86400",
          "X-Frame-Options": "SAMEORIGIN",
          "X-Content-Type-Options": "nosniff"
        }
      },
      {
        "src": "/cafe/.*",
        "dest": "/cafe/[slug]",
        "headers": {
          "Cache-Control": "public, max-age=7200, stale-while-revalidate=86400",
          "Strict-Transport-Security": "max-age=63072000; includeSubDomains; preload"
        }
      },
      {
        "src": "/producto/.*",
        "dest": "/producto/[id]",
        "headers": {
          "Cache-Control": "public, max-age=7200, stale-while-revalidate=86400",
          "X-Robots-Tag": "noindex, follow"
        }
      },
      {
        "src": "/checkout",
        "headers": {
          "Cache-Control": "no-cache, no-store, must-revalidate",
          "Permissions-Policy": "payment=()"
        }
      }
    ],
    "headers": [
      {
        "source": "/(.*)",
        "headers": [
          {
            "key": "Content-Security-Policy",
            "value": "default-src 'self' vercel.live *.vercel.app; script-src 'self' 'unsafe-inline' *.mercadopago.com; style-src 'self' 'unsafe-inline'; img-src 'self' data: *.amazonaws.com; font-src 'self'; frame-src *.mercadopago.com; connect-src 'self' api.mercadopago.com"
          },
          {
            "key": "Referrer-Policy",
            "value": "strict-origin-when-cross-origin"
          },
          {
            "key": "Permissions-Policy",
            "value": "geolocation=(), camera=(), microphone=()"
          },
          {
            "key": "X-XSS-Protection",
            "value": "1; mode=block"
          }
        ]
      }
    ],
    "redirects": [
      {
        "source": "/cafes",
        "destination": "/cafe",
        "permanent": true
      },
      {
        "source": "/productos",
        "destination": "/cafe",
        "permanent": true
      }
    ],
    "rewrites": [
      {
        "source": "/sitemap.xml",
        "destination": "/api/sitemap"
      }
    ],
    "regions": ["gru1"],
    "framework": "nextjs",
    "outputDirectory": "dist",
    "compress": true,
    "build": {
      "env": {
        "NEXT_PUBLIC_GA_ID": "@google-analytics-id",
        "MERCADOPAGO_API_KEY": "@mercadopago-api-key"
      }
    }
  }