{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/next"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Content-Encoding",
          "value": "br, gzip"
        },
        {
          "key": "X-Frame-Options",
          "value": "SAMEORIGIN"
        }
      ]
    },
    {
      "source": "/cafe/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=7200, stale-while-revalidate=86400"
        }
      ]
    }
  ],
  "redirects": [
    {
      "source": "/cafes",
      "destination": "/cafe",
      "permanent": true
    },
    {
      "source": "/productos",
      "destination": "/cafe",
      "permanent": true
    }
  ]
}